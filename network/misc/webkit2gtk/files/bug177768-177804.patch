-static BROTLI_INLINE int SafeReadBits(
-    BrotliBitReader* const br, uint32_t n_bits, uint32_t* val) {
-  if (n_bits != 0) {
-    return BrotliSafeReadBits(br, n_bits, val);
-  } else {
-    *val = 0;
-    return 1;
-  }
-}
