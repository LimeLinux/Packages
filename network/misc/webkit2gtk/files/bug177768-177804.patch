-      p2 = p1;
-      if (!safe) {
-        p1 = (uint8_t)ReadSymbol(hc, br);
-      } else {
-        uint32_t literal;
-        if (!SafeReadSymbol(hc, br, &literal)) {
-          result = BROTLI_NEEDS_MORE_INPUT;
-          goto saveStateAndReturn;
-        }
