-      goto CommandPostWrapCopy;
-    }
-    if (dst_end >= s->ringbuffer_size || src_end >= s->ringbuffer_size) {
-      /* At least one region wraps. */
-      goto CommandPostWrapCopy;
-    }
-    pos += i;
-    if (i > 16) {
-      if (i > 32) {
-        memcpy(copy_dst + 16, copy_src + 16, (size_t)(i - 16));
-      } else {
