    <Package>
        <Name>python-sphinx</Name>
        <RuntimeDependencies>
            <Dependency>docutils</Dependency>
            <Dependency>python-Pygments</Dependency>
            <Dependency>python-Jinja2</Dependency>
        </RuntimeDependencies>
        <Files>
            <Path fileType="executable">/usr/bin</Path>
